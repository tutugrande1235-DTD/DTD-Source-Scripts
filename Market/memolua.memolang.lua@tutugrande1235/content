local key = 94
memo.addhighl("local",key)
memo.addhighl("function",key)
memo.addhighl("end",key)
memo.addhighl("for",key)
memo.addhighl("in",key)
memo.addhighl("do",key)
memo.addhighl("while",key)
memo.addhighl("true",key)
memo.addhighl("false",key)
memo.addhighl("repeat",key)
memo.addhighl("until",key)
memo.addhighl("if",key)
memo.addhighl("then",key)
memo.addhighl("else",key)
memo.addhighl("elseif",key)
memo.addhighl("and",key)
memo.addhighl("or",key)
memo.addhighl("not",key)
memo.addhighl("goto",key)
memo.addhighl("break",key)
memo.addhighl("return",key)
memo.addhighl("nil",96)
memo.addhighl("(%d+)",96)
memo.addkeyopen("do")
memo.addkeyopen("then")
memo.addkeyopen("function")
memo.addkeyclose("end")